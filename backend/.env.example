# Recall API Configuration
RECALL_API_KEY=your_recall_api_key_here
RECALL_REGION=us-west-2
RECALL_WEBHOOK_URL=your_webhook_url_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Session Mode
SESSION_MODE=production

# Indicator Observability (Optional)
# Set to "true" to enable logging of all indicator evaluations
INDICATOR_OBSERVABILITY_ENABLED=false
# Directory where indicator logs will be saved
INDICATOR_LOG_PATH=observability_logs

# Application base URL for webhooks and media streaming
# This should be the publicly accessible URL of your backend
APP_BASE_URL=http://localhost:8000